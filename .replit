modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["freetype", "glibcLocales", "libxcrypt", "pkg-config", "xsimd"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "streamlit_app"

[[workflows.workflow]]
name = "streamlit_app"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

